html, body {
background-color:#ffffff;
height: 100%;
}

#normal {
  padding: 0 .4em .4em;  
  text-align: left;
  font-size: 1em;
  font: normal 13px verdana, arial, helvetica, sans-serif;
  /*color: #ff3333;*/
  background: transparent;
}

#p2R {
  /*padding: 0 .4em .4em;  */
  text-align: right;
  font-size: 1em;
  font-weight: bold;
  font: normal 13px verdana, arial, helvetica, sans-serif;
  text-transform: uppercase;
  color: #333333;
  background: transparent;
}

#p2c {
  /*padding: 0 .4em .4em;  */
  text-align: center;
  font-size: 1em;
  font-weight: bold;
  text-transform: uppercase;
  color: #333333;
  background: transparent;
}


table.normal {margin-left:auto; margin-right:auto; }

table.normal {font: 105% "Lucida Grande", "Lucida Sans Unicode", "Trebuchet MS", sans-serif;padding: 0; margin:auto; color: #000000; background: #F3F5F7;}

table.normal a {color: #3A4856; text-decoration: none; border-bottom: 1px solid #C6C8CB;}  

table.normal a:visited {color: #777;}

table.normal a:hover {color: #000;}  

table.normal caption {text-align: left; text-transform: uppercase;  padding-bottom: 10px; font: 200% "Lucida Grande", "Lucida Sans Unicode", "Trebuchet MS", sans-serif;}

table.normal thead th {background: #3A4856; padding: 15px 10px; color: #fff; text-align: left; font-weight: normal;}

table.normal tbody, table thead {border-left: 1px solid #EAECEE; border-right: 1px solid #EAECEE;}

table.normal tbody {border-bottom: 1px solid #EAECEE;}
                      
table.normal tbody td {padding: 10px; background: #EAECEE repeat-x; text-align: left;}

table.normal tbody th {padding: 10px; background: url("td_back.gif") repeat-x; text-align: left; font-weight:normal;}

table.normal tbody tr {background: #F3F5F7;}

table.normal tbody tr.odd {background: #F0F2F4;}

/*table.normal tbody  tr:hover {background: #EAECEE; color: #111;}*/

table.normal tbody  td:hover {background: #F3F5F7; color: #111;}

table.normal tbody  th:hover {background: #EAECEE; color: #111;}

table.normal tfoot td, table tfoot th, table tfoot tr {text-align: left; font: 100%  "Lucida Grande", "Lucida Sans Unicode", "Trebuchet MS", sans-serif; text-transform: uppercase; background: #fff; padding: 10px;}
